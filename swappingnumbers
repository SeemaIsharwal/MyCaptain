#include<stdio.h>

int main() {

int x , y;
 
    printf("Enter two numbers x and y");
    scanf("%d %d", &x, &y);     // user can enter two numbers
   
    // Code to swap 'x' and 'y' , let's say x=20, y=30
    x = x + y; // x now becomes 50
    y = x - y; // y becomes 20
    x = x - y; // x becomes 30
 
    printf("After Swapping: x = %d, y = %d", x, y);

return 0;
}
